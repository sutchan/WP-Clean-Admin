# Core settings
msgid "WP Clean Admin Settings"
msgstr "WP Clean Admin 设置"

msgid "Clean Admin"
msgstr "简洁后台"

msgid "You do not have sufficient permissions to access this page."
msgstr "您没有足够的权限访问此页面。"

msgid "General Settings"
msgstr "常规设置"

msgid "Configure the appearance and behavior of your admin dashboard."
msgstr "配置后台界面的外观和行为。"

msgid "Menu Customization"
msgstr "菜单自定义"

msgid "Drag and drop to reorder menu items"
msgstr "拖放以重新排序菜单项"

msgid "Reset to Default"
msgstr "重置为默认"

msgid "Are you sure you want to reset all menu settings to default?"
msgstr "您确定要将所有菜单设置重置为默认值吗？"


# Visual style translations
msgid "Visual Style"
msgstr "视觉风格"

msgid "Customize the overall visual theme and colors."
msgstr "自定义整体视觉主题和颜色。"

msgid "Theme Style"
msgstr "主题风格"

msgid "Default (Flat & Clean)"
msgstr "默认(扁平简洁)"

msgid "Light Blue Gray"
msgstr "浅蓝灰色"

msgid "Mint Green"
msgstr "薄荷绿"

msgid "Dark Mode"
msgstr "暗色模式"

msgid "Custom Colors"
msgstr "自定义颜色"

msgid "Choose a predefined theme or select \"Custom Colors\" to define your own."
msgstr "选择预定义主题或选择\"自定义颜色\"来定义您自己的主题。"

msgid "Primary Color"
msgstr "主色调"

msgid "Choose the primary accent color for links, buttons, and active states."
msgstr "选择链接、按钮和活动状态的主色调。"

msgid "Background Color"
msgstr "背景色"

msgid "Choose the main background color of the admin area."
msgstr "选择后台区域的主要背景色。"

msgid "Text Color"
msgstr "文字颜色"

msgid "Choose the default text color."
msgstr "选择默认文字颜色。"

msgid "Shadow Style"
msgstr "阴影风格"

msgid "None"
msgstr "无"

msgid "Subtle (Default)"
msgstr "微妙(默认)"

msgid "Choose the shadow style for elements like post boxes and buttons."
msgstr "为文章框和按钮等元素选择阴影风格。"

# Layout translations
msgid "Layout & Typography"
msgstr "布局与排版"

msgid "Show menu icons"
msgstr "显示菜单图标"

msgid "Adjust layout density, element rounding, and font styles."
msgstr "调整布局密度、元素圆角和字体样式。"

msgid "Layout Density"
msgstr "布局密度"

msgid "Compact"
msgstr "紧凑"

msgid "Standard (Default)"
msgstr "标准(默认)"

msgid "Spacious"
msgstr "宽松"

msgid "Adjust the spacing and padding of elements in the admin interface."
msgstr "调整后台界面中元素的间距和内边距。"

msgid "Border Radius"
msgstr "边框圆角"

msgid "None (Sharp Corners)"
msgstr "无(直角)"

msgid "Small (4px, Default)"
msgstr "小(4px,默认)"

msgid "Large (8px, Rounded)"
msgstr "大(8px,圆角)"

msgid "Choose the border-radius style for various elements."
msgstr "为各种元素选择边框圆角样式。"

msgid "Font Stack"
msgstr "字体堆栈"

msgid "System Default"
msgstr "系统默认"

msgid "Select the font stack to be used in the admin area."
msgstr "选择在后台区域使用的字体堆栈。"

msgid "Font Size"
msgstr "字体大小"

msgid "Small"
msgstr "小"

msgid "Medium (Default)"
msgstr "中(默认)"

msgid "Large"
msgstr "大"

msgid "Adjust the base font size for the admin interface."
msgstr "调整后台界面的基础字体大小。"

msgid "Icon Style"
msgstr "图标风格"

msgid "WordPress Dashicons"
msgstr "WordPress Dashicons"

msgid "Choose the icon set for the admin interface."
msgstr "为后台界面选择图标集。"

