{
    "name": "sutchan/wp-clean-admin",
    "description": "WordPress Admin Cleanup and Optimization Plugin",
    "type": "wordpress-plugin",
    "version": "1.8.0",
    "keywords": [
        "wordpress",
        "admin",
        "cleanup",
        "optimization",
        "security"
    ],
    "homepage": "https://github.com/sutchan/Open-PNR",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Sut",
            "email": "",
            "homepage": "https://github.com/sutchan",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "composer/installers": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "wp-coding-standards/wpcs": "^3.0",
        "phpstan/phpstan": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "WPCleanAdmin\\": "wpcleanadmin/includes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WPCleanAdmin\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpcs": "phpcs --standard=WordPress wpcleanadmin/",
        "phpstan": "phpstan analyze wpcleanadmin/",
        "lint": [
            "@phpcs",
            "@phpstan"
        ]
    }
}

